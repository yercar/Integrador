                                                <Label x:Name="LblValOmisionDim2" Content="Valor por omisión:" Width="112"></Label>
                                                <TextBox x:Name="TxtValOmisionDim2" Width="80"></TextBox>
                                            </WrapPanel>
                                            <c1:C1FlexGrid>
                                                <c1:C1FlexGrid.Columns>
                                                    <c1:Column Header="Valor ST"></c1:Column>
                                                    <c1:Column Header="Valor ERP"></c1:Column>
                                                </c1:C1FlexGrid.Columns>
                                            </c1:C1FlexGrid>
                                        </StackPanel>
                                    </c1:C1TabItem.Content>
                                </c1:C1TabItem>
                                <c1:C1TabItem Header="General">
                                    <c1:C1TabItem.Content>
                                        <StackPanel>
                                            <WrapPanel>
                                                <Label x:Name="LblValorStDim2" Content="Valor ST:" Width="100"></Label>
                                                <TextBox x:Name="TxtValorStDim2" Width="290"></TextBox>
                                            </WrapPanel>
                                            <WrapPanel>
                                                <Label x:Name="LblValorErpDim2" Content="Valor ERP:" Width="100"></Label>
                                                <TextBox x:Name="TxtValorErpDim2" Width="290"></TextBox>
                                            </WrapPanel>
                                            <Grid Height="79">
                                                <Button x:Name="BtnNuevaDim2" Content="Nuevo" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top" Width="104" Height="30"/>
                                                <Button x:Name="BtnEliminarDim2" Content="Eliminar" HorizontalAlignment="Left" Margin="146,39,0,0" VerticalAlignment="Top" Width="104" Height="30"/>
                                                <Button x:Name="BtnLimpiarDim2" Content="Limpiar" HorizontalAlignment="Left" Margin="282,39,0,0" VerticalAlignment="Top" Width="104" Height="30"/>
                                            </Grid>
                                        </StackPanel>
                                    </c1:C1TabItem.Content>
                                </c1:C1TabItem>
                            </c1:C1TabControl>

                        </StackPanel>
                    </c1:C1TabItem.Content>
                </c1:C1TabItem>

                <c1:C1TabItem Header="Dimensión 3">
                    <c1:C1TabItem.Content>
                        <StackPanel>
                            <c1:C1TabControl Height="328" Margin="10,0">
                                <c1:C1TabItem Header="Visión General">
                                    <c1:C1TabItem.Content>
                                        <StackPanel>
                                            <WrapPanel>
                                                <Label x:Name="LblDescripcionDim3" Content="Descripción:" Width="100"></Label>
                                                <TextBox x:Name="TxtDescripcionDim3" Width="290"></TextBox>
                                            </WrapPanel>
                                            <WrapPanel>
                                                <CheckBox x:Name="ChkValidaDim3" Content="Valida" Width="80"></CheckBox>
                                                <CheckBox x:Name="ChkUsaMapeoDim3" Content="Utiliza mapeo" Width="107"></CheckBox>
                                                <Label x:Name="LblValOmisionDim3" Content="Valor por omisión:" Width="112"></Label>
                                                <TextBox x:Name="TxtValOmisionDim3" Width="80"></TextBox>
                                            </WrapPanel>
                                            <c1:C1FlexGrid>
                                                <c1:C1FlexGrid.Columns>
                                                    <c1:Column Header="Valor ST"></c1:Column>
                                                    <c1:Column Header="Valor ERP"></c1:Column>
                                                </c1:C1FlexGrid.Columns>
                                            </c1:C1FlexGrid>
                                        </StackPanel>
                                    </c1:C1TabItem.Content>
                                </c1:C1TabItem>
                                <c1:C1TabItem Header="General">
                                    <c1:C1TabItem.Content>
                                        <StackPanel>
                                            <WrapPanel>
                                                <Label x:Name="LblValorStDim3" Content="Valor ST:" Width="100"></Label>
                                                <TextBox x:Name="TxtValorStDim3" Width="290"></TextBox>
                                            </WrapPanel>
                                            <WrapPanel>
                                                <Label x:Name="LblValorErpDim3" Content="Valor ERP:" Width="100"></Label>
                                                <TextBox x:Name="TxtValorErpDim3" Width="290"></TextBox>
                                            </WrapPanel>
                                            <Grid Height="79">
                                                <Button x:Name="BtnNuevaDim3" Content="Nuevo" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top" Width="104" Height="30"/>
                                                <Button x:Name="BtnEliminarDim3" Content="Eliminar" HorizontalAlignment="Left" Margin="146,39,0,0" VerticalAlignment="Top" Width="104" Height="30"/>
                                                <Button x:Name="BtnLimpiarDim3" Content="Limpiar" HorizontalAlignment="Left" Margin="282,39,0,0" VerticalAlignment="Top" Width="104" Height="30"/>
                                            </Grid>
                                        </StackPanel>
                                    </c1:C1TabItem.Content>
                                </c1:C1TabItem>
                            </c1:C1TabControl>
                        </StackPanel>
                    </c1:C1TabItem.Content>
                </c1:C1TabItem>

            </c1:C1TabControl>
        </Grid>

    </Grid>
</UserControl>
