<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml" x:Class="GRW.Integrador.Vista.TodosLosUsuarios" 
             mc:Ignorable="d" 
             d:DesignHeight="499.388" d:DesignWidth="480">
    <Grid Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}">
        <Label x:Name="LblTituloTodosLosUsuarios" Content="Todos los usuarios" HorizontalAlignment="Left" Margin="24,27,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Semibold"/>
        <c1:C1FlexGrid x:Name="GridTodosLosUsuarios" HorizontalAlignment="Left" Margin="24,71,0,0" VerticalAlignment="Top" Height="406" Width="432">
            <c1:C1FlexGrid.Columns>
                <c1:Column Header="Nombre de usuario" AllowDragging="False" Width="200"></c1:Column>
                <c1:Column Header="Fecha de alta" AllowDragging="False" Width="200"></c1:Column>
            </c1:C1FlexGrid.Columns>
        </c1:C1FlexGrid>


    </Grid>
</UserControl>
